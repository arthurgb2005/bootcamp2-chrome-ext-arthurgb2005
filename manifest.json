{
  "manifest_version": 3,
  "name": "Extensão Bootcamp",
  "version": "1.0.0",
  "description": "Extensão exemplo para o bootcamp (popup + background).",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": []
}

